   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.integrate import solve_ivp\n",
    "\n",
    "m = 1\n",
    "l = 0.5\n",
    "J = 0.05\n",
    "b = 0\n",
    "g = 9.81\n",
    "f = 0.2\n",
    "\n",
    "#define arrays A, B and C of a linear system\n",
    "A = -b/(m*l*l + J)\n",
    "\n",
    "B = -m*g*l/(m*l*l + J)\n",
    "\n",
    "C = 1/(m*l*l + J)\n",
    "\n",
    "#define time-dependent input function. Here, unit step is defined\n",
    "def force(t):\n",
    "    return 0\n",
    "\n",
    "#define dynamic system.\n",
    "def deg1(t, x):\n",
    "    dx1 = x[1];\n",
    "    dx2 = x[1] * A + B * np.sin(x[0]) + C * force(t)\n",
    "       \n",
    "    return [dx1, dx2]\n",
    "\n",
    "def deg2(t, x):\n",
    "    dx1 = x[1];\n",
    "    dx2 = x[1] * (-b/(m*l*l + J)) + B * x[0] + C * force(t)\n",
    "       \n",
    "    return [dx1, dx2]\n",
    "\n",
    "\n",
    "#simulate the dynamic system, pass a system deg1, time of simulation and initial conditions\n",
    "res1 = solve_ivp(deg1, [0,30], [np.pi/18,0], rtol=1e-10) #arguments rtol and atol sets calculation tolerance\n",
    "\n",
    "res2 = solve_ivp(deg2, [0,30], [np.pi/18,0], rtol=1e-10)\n",
    "\n",
    "#calculate output based on the obtained state\n",
    "y = res.y[0]\n",
    "\n",
    "#plot results\n",
    "plt.plot(res1.t, res1.y[0])\n",
    "plt.plot(res2.t, res2.y[0])\n",
    "\n",
    "plt.savefig(\"nazwapliku.pdf\", format = 'pdf')\n"
   ]
