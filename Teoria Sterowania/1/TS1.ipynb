     "import numpy as np\n",
     "import matplotlib.pyplot as plt\n",
     "from scipy.integrate import solve_ivp\n",
     "\n",
     "m = 1\n",
     "l = 0.5\n",
     "J = 0.05\n",
     "b = 0\n",
     "g = 9.81\n",
     "f = 0.2\n",
     "\n",
     "#define arrays A, B and C of a linear system\n",
     "A = -b/(m*l*l + J)\n",
     "\n",
     "B = -m*g*l/(m*l*l + J)\n",
     "\n",
     "C = 1/(m*l*l + J)\n",
     "\n",
     "#define time-dependent input function. Here, unit step is defined\n",
     "def force(t):\n",
     "    return 0\n",
     "\n",
     "\n",
     "#define dynamic system.\n",
     "def deg1(t, x):\n",
     "    dx1 = x[1];\n",
     "    dx2 = x[1] * A + np.sin(x[0]) * B + C * force(t)\n",
     "       \n",
     "    return [dx1, dx2]\n",
     "\n",
     "#simulate the dynamic system, pass a system deg1, time of simulation and initial conditions\n",
     "res = solve_ivp(deg1, [0,60], [np.pi/2,0], rtol=1e-10) #arguments rtol and atol sets calculation tolerance\n",
     "\n",
     "#calculate output based on the obtained state\n",
     "#y = res.y[0]\n",
     "\n",
     "#plot results\n",
     "plt.plot(res.t, res.y[0])\n",
     "plt.plot(res.t, res.y[1])\n",
     "\n",
     "#plt.savefig(\"filepath.pdf\", format = 'pdf')\n",
     "\n",
     "\n",
     "#new simulation\n",
     "\n",
     "m = 1\n",
     "l = 0.5\n",
     "J = 0.05\n",
     "b = 0\n",
     "g = 9.81\n",
     "f = 0.2\n",
     "\n",
     "#define arrays A, B and C of a linear system\n",
     "A = -b/(m*l*l + J)\n",
     "\n",
     "B = -m*g*l/(m*l*l + J)\n",
     "\n",
     "C = 1/(m*l*l + J)\n",
     "\n",
     "#define time-dependent input function. Here, unit step is defined\n",
     "def force(t):\n",
     "    return 0\n",
     "\n",
     "#define dynamic system.\n",
     "def deg1(t, x):\n",
     "    dx1 = x[1];\n",
     "    dx2 = x[1] * A + B * np.sin(x[0]) + C * force(t)\n",
     "       \n",
     "    return [dx1, dx2]\n",
     "\n",
     "def deg2(t, x):\n",
     "    dx1 = x[1];\n",
     "    dx2 = x[1] * (-b/(m*l*l + J)) + B * x[0] + C * force(t)\n",
     "       \n",
     "    return [dx1, dx2]\n",
     "\n",
     "\n",
     "#simulate the dynamic system, pass a system deg1, time of simulation and initial conditions\n",
     "res1 = solve_ivp(deg1, [0,30], [np.pi/18,0], rtol=1e-10) #arguments rtol and atol sets calculation tolerance\n",
     "\n",
     "res2 = solve_ivp(deg2, [0,30], [np.pi/18,0], rtol=1e-10)\n",
     "\n",
     "#calculate output based on the obtained state\n",
     "y = res.y[0]\n",
     "\n",
     "#plot results\n",
     "plt.plot(res1.t, res1.y[0])\n",
     "plt.plot(res2.t, res2.y[0])\n",
     "\n",
     "plt.savefig(\"nazwapliku.pdf\", format = 'pdf')\n",
     "\n",
     "\n",
     "#new simulation\n",
     "\n",
     "#initialize constant values\n",
     "b = 0\n",
     "k = 1\n",
     "m = 1\n",
     "\n",
     "#define arrays A, B and C of a linear system\n",
     "A = [[0, 1],\n",
     "    [-k/m, -b/m]]\n",
     "\n",
     "B = [0,\n",
     "    1/m]\n",
     "\n",
     "C = [1, 0]\n",
     "\n",
     "#define time-dependent input function. Here, unit step is defined\n",
     "def force(t):\n",
     "    return np.sin(2*t)\n",
     "\n",
     "#define dynamic system.\n",
     "def deg1(t, x):\n",
     "    dx1 = x[1];\n",
     "    dx2 = x[0] * A[1][0] + x[1] * A[1][1] + B[1] * force(t)\n",
     "       \n",
     "    return [dx1, dx2]\n",
     "\n",
     "#simulate the dynamic system, pass a system deg1, time of simulation and initial conditions\n",
     "res = solve_ivp(deg1, [0,60], [0,0], rtol=1e-10) #arguments rtol and atol sets calculation tolerance\n",
     "\n",
     "#calculate output based on the obtained state\n",
     "y = res.y[0]\n",
     "\n",
     "#plot results\n",
     "plt.plot(res.t, res.y[0], label=\"ksi\")\n",
     "plt.plot(res.t, res.y[1], label=\"ksi'\")\n",
     "plt.legend(loc=4)\n",
     "plt.xlabel('czas')\n",
     "plt.savefig(\"filepath.pdf\", format = 'pdf')\n",
     "\n",
     "\n",
     "#new simulation\n",
     "\n",
     "#initialize constant values\n",
     "R_1 = 0.2\n",
     "L = 0.1\n",
     "C = 0.05\n",
     "A = 2\n",
     "\n",
     "\n",
     "#define time-dependent input function. Here, unit step is defined\n",
     "def force(t):\n",
     "    #return np.array([[0]]); #return as an array with single element\n",
     "    return A*1\n",
     "   \n",
     "\n",
     "#define dynamic system.\n",
     "def deg1(t, x):\n",
     "    dx1 = x[0] * (-R_1/L) + x[1] * (-1/L) + (1/L) * force(t)\n",
     "    dx2 = x[0] * (1/C) - (5-force(t))/(C*(force(t)*0.25)) * x[1]\n",
     "       \n",
     "    return [dx1, dx2]\n",
     "\n",
     "#simulate the dynamic system, pass a system deg1, time of simulation and initial conditions\n",
     "res = solve_ivp(deg1, [0,2], [0,0], rtol=1e-10) #arguments rtol and atol sets calculation tolerance\n",
     "\n",
     "\n",
     "#calculate output based on the obtained state\n",
     "\n",
     "y = res.y[0] - res.y[1]*0.25/(5-res.y[1]);\n",
     "\n",
     "#plot results\n",
     "\n",
     "plt.plot(res.t,res.y[0], label='i_1 [A]')\n",
     "plt.plot(res.t, res.y[1], label='u_c [V]')\n",
     "plt.xlabel('czas (t)')\n",
     "plt.legend()\n",
     "\n",
     "#new simulation\n",
     "\n",
     "# initialize constant values\n",
     "b = 0\n",
     "k = 1\n",
     "m = 1\n",
     "\n",
     "# define arrays A, B and C of a linear system\n",
     "A = np.array([[0, 1],\n",
     "              [-k / m, -b / m]])\n",
     "\n",
     "B = np.array([[0],\n",
     "              [1 / m]])\n",
     "\n",
     "C = np.array([[1, 0]])\n",
     "\n",
     "\n",
     "# define time-dependent input function. Here, unit step is defined\n",
     "def force(t):\n",
     "    return np.array([[1]])  # return as an array with single element\n",
     "\n",
     "\n",
     "# define dynamic system.\n",
     "def deg1(t, x):\n",
     "    x = np.array([x]).T  # for compatibility with a solver, convert x to matrix and get transpose\n",
     "\n",
     "    y = A @ x + B @ force(t);  # calculate state equation\n",
     "    # note: matrix multiplication is done using @ operator\n",
     "\n",
     "    return np.ndarray.tolist(y.T[0])  # for compatibility with a solver, transpose x and convert into list\n",
     "\n",
     "\n",
     "# simulate the dynamic system, pass a system deg1, time of simulation and initial conditions\n",
     "res = solve_ivp(deg1, [0, 10], [0, 0], rtol=1e-10)  # arguments rtol and atol sets calculation tolerance\n",
     "\n",
     "# calculate output based on the obtained state\n",
     "y = C @ np.array(res.y);\n",
     "y = np.ndarray.tolist(y[0].T);\n",
     "\n",
     "# plot results\n",
     "plt.plot(res.t, res.y[0])\n",
     "plt.plot(res.t, res.y[1])\n",
     "# plt.savefig(\"filepath.pdf\", format = 'pdf')\n",
     "\n"
